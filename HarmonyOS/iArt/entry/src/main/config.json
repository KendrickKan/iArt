{
  "app": {
    "bundleName": "com.example.iart",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0",
    }
  },
  "deviceConfig": {
    "default": {
      "directLaunch": false,
      "network": {
        "cleartextTraffic": true,
        "securityConfig": {
          "domainSettings": {
            "cleartextPermitted": true,
            "domains": [
              {
                "subdomains": true,
                "name": "127.0.0.1"
              },
              {
                "subdomains": true,
                "name" : "121.36.100.95"
              }
            ]
          }
        }
      }

    }
  },

  "module": {
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "name": "com.example.iart.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "js": [
      {
        "pages": [
          "pages/page2/page2",
          "pages/page/page",
          "pages/user/user",
          "pages/detail/detail",
          "pages/page3/page3"
        ],
        "name": "default",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": true
        }
      }
    ],
    "mainAbility": "com.example.iart.MainAbility",
    "metaData": {
      "customizeData": [
        {
          "name": "hwc-theme",
          "value": "androidhwext:style/Theme.Emui.Light.NoTitleBar",
          "extra": ""
        }
      ]
    },
    "name": ".MyApplication",
    "package": "com.example.iart",
    "reqPermissions": [
      {
        "name": "ohos.permission.GET_NETWORK_INFO"
      },
      {
        "name": "ohos.permission.INTERNET"
      },
      {
        "name": "ohos.permission.SET_NETWORK_INFO"
      }
    ]
  }
}